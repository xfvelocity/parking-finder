{
  "name": "parking-finder",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prettier": "prettier . --write",
    "sync": "ionic capacitor sync",
    "open:android": "ionic capacitor open android",
    "open:ios": "ionic capacitor open ios",
    "run:ios": "ionic capacitor run ios -l --external",
    "run:android": "ionic capacitor run android -l --external",
    "appflow:build": "if [ \"$CI_PLATFORM\" != \"web\" ]; then npx trapeze run appflow.yml -y --$CI_PLATFORM; fi && npm run build"
  },
  "dependencies": {
    "@capacitor/android": "6.2.0",
    "@capacitor/app": "6.0.2",
    "@capacitor/core": "6.2.0",
    "@capacitor/geolocation": "^6.0.2",
    "@capacitor/haptics": "6.0.2",
    "@capacitor/ios": "6.2.0",
    "@capacitor/keyboard": "6.0.3",
    "@capacitor/status-bar": "6.0.2",
    "@ionic/vue": "^8.4.0",
    "@ionic/vue-router": "^8.4.0",
    "@trapezedev/configure": "^7.1.3",
    "@types/google.maps": "^3.58.1",
    "@vueuse/core": "^11.3.0",
    "axios": "^1.7.7",
    "ionicons": "^7.4.0",
    "pinia": "2.2.6",
    "pinia-plugin-persistedstate": "^4.1.3",
    "vue": "^3.5.13",
    "vue-inline-svg": "^3.1.4",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "6.2.0",
    "@vitejs/plugin-vue": "^5.2.0",
    "prettier": "^3.3.3",
    "sass": "^1.81.0",
    "typescript": "5.6.3",
    "vite": "^5.4.11",
    "vite-plugin-pwa": "^0.21.0",
    "vue-tsc": "^2.1.10"
  }
}
