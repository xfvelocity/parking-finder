{
  "name": "parking-finder",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "sync": "ionic capacitor sync",
    "open:android": "ionic capacitor open android",
    "open:ios": "ionic capacitor open ios",
    "run:ios": "ionic capacitor run ios -l --external",
    "run:android": "ionic capacitor run android -l --external",
    "appflow:build": "if [ \"$CI_PLATFORM\" != \"web\" ]; then npx trapeze run appflow.yml -y --$CI_PLATFORM; fi && npm run build",
    "prettier": "prettier . --write",
    "lint": "vue-tsc --noEmit && eslint . --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/app": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/geolocation": "^8.0.0",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/ios": "^8.0.0",
    "@capacitor/keyboard": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@ionic/vue": "^8.7.13",
    "@ionic/vue-router": "^8.7.13",
    "@trapezedev/configure": "^7.1.3",
    "@types/google.maps": "^3.58.1",
    "@vueuse/core": "^13.9.0",
    "axios": "^1.13.2",
    "ionicons": "7.4.0",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "vue": "^3.5.25",
    "vue-inline-svg": "3.1.4",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^8.0.0",
    "@types/node": "^22.19.3",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "8.56.0",
    "eslint-plugin-vue": "^10.6.2",
    "husky": "^9.1.7",
    "prettier": "^3.7.4",
    "sass": "^1.97.0",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-plugin-pwa": "^1.2.0",
    "vue-tsc": "^2.2.12"
  }
}