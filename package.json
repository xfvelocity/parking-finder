{
  "name": "parking-finder",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prettier": "prettier . --write",
    "sync": "ionic capacitor sync",
    "open:android": "ionic capacitor open android",
    "open:ios": "ionic capacitor open ios",
    "run:ios": "ionic capacitor run ios -l --external",
    "run:android": "ionic capacitor run android -l --external",
    "appflow:build": "if [ \"$CI_PLATFORM\" != \"web\" ]; then npx trapeze run appflow.yml -y --$CI_PLATFORM; fi && npm run build"
  },
  "dependencies": {
    "@capacitor/android": "^7.1.0",
    "@capacitor/app": "^7.0.0",
    "@capacitor/core": "^7.1.0",
    "@capacitor/geolocation": "^7.1.1",
    "@capacitor/haptics": "^7.0.0",
    "@capacitor/ios": "^7.1.0",
    "@capacitor/keyboard": "^7.0.0",
    "@capacitor/status-bar": "^7.0.0",
    "@ionic/vue": "^8.5.0",
    "@ionic/vue-router": "^8.5.0",
    "@trapezedev/configure": "^7.1.3",
    "@types/google.maps": "^3.58.1",
    "@vueuse/core": "^13.0.0",
    "axios": "^1.8.3",
    "ionicons": "^7.4.0",
    "pinia": "3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "vue": "^3.5.13",
    "vue-inline-svg": "3.1.4",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^7.1.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "prettier": "^3.5.3",
    "sass": "^1.85.1",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vite-plugin-pwa": "^0.21.1",
    "vue-tsc": "^2.2.8"
  }
}
